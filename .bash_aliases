# ls
alias lsa="ls -a"
alias lsl="ls -l"
alias lsla="ls -la"
alias lslah="ls -lah"

# git
alias gs="git status"
alias ga="git add"
alias gc="git commit"
alias gac="git add . && git commit"
alias gph="git push"
alias gpl="git pull"
alias gf="git fetch"
alias gd="git diff"
alias gl="git log"

# tar & gzip
alias mktarball="tar -czvf"
alias untarball="tar -zxvf"
