[filter "lfs"]
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
  clean = git-lfs clean -- %f
[user]
  name = ##USERNAME##
  email = ##USER_ID##+##USERNAME##@users.noreply.github.com
[core]
  editor = ##GIT_EDITOR##
[init]
  defaultBranch = main
[url "git@github.com:"]
  insteadOf = https://github.com/
